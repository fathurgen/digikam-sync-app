  "packagerConfig": {
    "asar": true,
    "asarUnpack": [
      "**/node_modules/sharp/**/*",
      "**/node_modules/@img/**/*",
      "**/node_modules/better-sqlite3/**/*",
      "dist/workers/**/*",
      "src/workers/**/*"
